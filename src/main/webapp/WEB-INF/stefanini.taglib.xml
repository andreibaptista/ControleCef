<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib>
    <namespace>http://cf.com.br/tags</namespace>
    
    <!-- Tag Responsável por definir o titulo dos conteúdos página -->
    <tag>
		<tag-name>title</tag-name>
		<source>tags/title-content.xhtml</source>
    </tag>
    
    <!-- Tag Responsável por definir o titulo dos conteúdos página -->
    <tag>
		<tag-name>mensagens</tag-name>
		<source>tags/messages.xhtml</source>
    </tag>
    
    <!-- Tag Responsável por definir o titulo dos conteúdos página -->
    <tag>
		<tag-name>loadDialog</tag-name>
		<source>tags/loading.xhtml</source>
    </tag>
    
    <tag>
        <tag-name>virtualKeyboard</tag-name>
        <component>
			<component-type>VirtualKeyboard</component-type>
			<renderer-type>br.com.stefanini.cbf.components.VirtualKeyboardRenderer</renderer-type>
		</component>
    </tag>
</facelet-taglib>